{% layout 'layout.liquid' %}
{% block content %}
<div class="container-md">
    <article>
        <h2>
            {{ binomSet.title }}
        </h2>
        <h3>"Success": {{ binomSet.p }}</h3>
        <h3>"Failure": {{ binomSet.q }}</h3>
        {% if binomSet.owner == userId %}
        <div class="btn-group">

            <a href="/binomsets/{{ binomSet.id }}/edit" class="btn btn-primary">Edit</a>
            <form action="/binomsets/{{ binomSet.id }}?_method=DELETE" method="POST">
                <input type="submit" value="Delete" class="btn btn-danger">
            </form>
        </div>
        {% endif %}
    </article>
    {% render '../datasets/data.liquid', set: binomSet %}
    {% render '../datasets/notes.liquid', type:"binom", set: binomSet, loggedIn: loggedIn %}

</div>
{% endblock %}