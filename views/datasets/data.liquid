{% block content %}
<div class="container-fluid">
    <a class="btn btn-primary" data-toggle="collapse" href="#data-table" role="button" aria-expanded="false" aria-controls="data-table">
        View Data
    </a>
    <div class="collapse" id="data-table">
        <table class="table table-sm">
            <tr>
                <th scope="col">Value</th>
            </tr>
            {% for value in set.values %}
                <tr>
                    <td>
                        {% if set.p %}
                            {% if value == true %}
                                {{ set.p }}
                            {% else %}
                                {{ set.q }}
                            {% endif %}
                        {% else %}
                            {{ value }}
                        {% endif %}
                    </td>
                </tr>
            {% endfor %}
        </table>
    </div>
</div>
{% endblock %}